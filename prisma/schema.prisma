generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Category {
  id            String   @id @default(uuid())
  name          String
  image         String
  description   String
  taxApplicable Boolean  @default(false)
  taxNumber     Float
  taxType       String
  createdAt     DateTime @default(now())
  updatedAt     DateTime @updatedAt
}
